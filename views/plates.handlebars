<div class="container">
    <div class="row">
        <div class="col text-center">
            <h3>There are currently <span class="text-success ">{{counter}}</span> license plates in the database</h3>
        </div>
    </div>
    <div class="w-100"></div>
    <div class="row">
        <div class="col">
            <h5>All the plates will be represented by the Town in a table
                </h>
        </div>
    </div>
    <div class="w-100"></div>
    <div class="row">
        <table class="table mt-5">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">Location</th>
                    <th scope="col">Plate</th>
                    <th scope="col">Status</th>
                    <th scope="col">Change status</th>
                </tr>
            </thead>
            <tbody>
                {{#each listData}}
                <tr>
                    <td>{{town_name}}</td>
                    <td>{{plates}}</td>
                    <td>{{status}}</td>
                    <td><a href="/allPlates/{{status}}">Switch</a></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>

</div>